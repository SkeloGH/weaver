{
  "ignore":[
    "**/__tests__/**",
    "**/*.test.js",
  ],
  "presets": [
    [
      "@babel/preset-env", {
      "debug": true,
      "targets": {
        "node": "current"
      },
      "useBuiltIns": "usage",
      "corejs": { "version": 3, "proposals": true }
    }]
  ],
  "plugins": ["@babel/plugin-proposal-class-properties"]
}