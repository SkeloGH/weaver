{
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "env": {
        "jest": true,
        "browser": false,
        "es6": true
    },
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [
                    ".js",
                    ".json",
                    ".jsx",
                    ".ts",
                    ".tsx"
                ]
            }
        }
    },
    "rules": {
        "no-underscore-dangle": "off",
        "no-multi-str": "off"
    }
}